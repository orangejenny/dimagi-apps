<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Remove Maternal Indicators</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/97C564A5-DD4E-40F2-A1D3-ACBBD56412C3" uiVersion="1" version="282" name="Remove Maternal Indicators">
					<question1/>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><close/></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/>
			<bind nodeset="/data/question1"/>
			<itext>
				<translation lang="en" default="">
					<text id="question1-label">
						<value>Remove this measurement?</value>
					</text>
					<text id="question1-yes-label">
						<value>yes</value>
					</text>
					<text id="question1-no-label">
						<value>no</value>
					</text>
				</translation>
				<translation lang="hin">
					<text id="question1-label">
						<value>Remove this measurement?</value>
					</text>
					<text id="question1-yes-label">
						<value>yes</value>
					</text>
					<text id="question1-no-label">
						<value>no</value>
					</text>
				</translation>
			</itext>
		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><bind calculate="instance('commcaresession')/session/data/case_id" nodeset="/data/case/@case_id"/><bind nodeset="/data/case/close" relevant="/data/question1 = 'yes'"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/></model>
	</h:head>
	<h:body>
		<select1 ref="/data/question1">
			<label ref="jr:itext('question1-label')"/>
			<item>
				<label ref="jr:itext('question1-yes-label')"/>
				<value>yes</value>
			</item>
			<item>
				<label ref="jr:itext('question1-no-label')"/>
				<value>no</value>
			</item>
		</select1>
	</h:body>
</h:html>