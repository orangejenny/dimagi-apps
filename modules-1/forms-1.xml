<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Update Case</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/A4B75F2A-9F01-4596-9A86-BD01CD2971D0" uiVersion="1" version="213" name="Update Case">
					<name/>
					<location/>
					<value/>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><update><case_name/><location/><value/></update></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/><cc:location/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/><instance id="casedb" src="jr://instance/casedb"/>
			<bind nodeset="/data/name" type="xsd:string"/>
			<bind nodeset="/data/location" type="geopoint"/>
			<bind nodeset="/data/value"/>
			<itext>
				<translation lang="en" default="">
					<text id="name-label">
						<value>Case name?</value>
					</text>
					<text id="location-label">
						<value>Where are you?</value>
					</text>
					<text id="value-label">
						<value>Case value?</value>
					</text>
					<text id="value-yes-label">
						<value>yes</value>
					</text>
					<text id="value-no-label">
						<value>no</value>
					</text>
				</translation>
			</itext>
		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><bind calculate="instance('commcaresession')/session/data/case_id" nodeset="/data/case/@case_id"/><bind calculate="/data/name" nodeset="/data/case/update/case_name" relevant="count(/data/name) &gt; 0"/><bind calculate="/data/location" nodeset="/data/case/update/location" relevant="count(/data/location) &gt; 0"/><bind calculate="/data/value" nodeset="/data/case/update/value" relevant="count(/data/value) &gt; 0"/><setvalue event="xforms-ready" ref="/data/location" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/location"/><setvalue event="xforms-ready" ref="/data/name" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/case_name"/><setvalue event="xforms-ready" ref="/data/value" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/value"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/><orx:pollsensor event="xforms-ready" ref="/data/meta/location"/><bind nodeset="/data/meta/location" type="geopoint"/></model>
	</h:head>
	<h:body>
		<input ref="/data/name">
			<label ref="jr:itext('name-label')"/>
		</input>
		<input ref="/data/location">
			<label ref="jr:itext('location-label')"/>
		</input>
		<select1 ref="/data/value">
			<label ref="jr:itext('value-label')"/>
			<item>
				<label ref="jr:itext('value-yes-label')"/>
				<value>yes</value>
			</item>
			<item>
				<label ref="jr:itext('value-no-label')"/>
				<value>no</value>
			</item>
		</select1>
	</h:body>
</h:html>