<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Add Song</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/B520859B-BE3D-4DAB-B8E9-585C3E1019C5" uiVersion="1" version="57" name="Add Song">
					<basics>
						<name/>
						<artist/>
					</basics>
					<opinions>
						<rating/>
						<mood/>
						<energy/>
					</opinions>
					<plays/>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><create><case_name/><owner_id/><case_type>song</case_type></create><update><artist/><energy/><mood/><plays/><rating/></update></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/>
			<bind nodeset="/data/basics"/>
			<bind nodeset="/data/basics/name" type="xsd:string" required="true()"/>
			<bind nodeset="/data/basics/artist" type="xsd:string"/>
			<bind nodeset="/data/opinions"/>
			<bind nodeset="/data/opinions/rating"/>
			<bind nodeset="/data/opinions/mood"/>
			<bind nodeset="/data/opinions/energy"/>
			<bind nodeset="/data/plays" calculate="0"/>
			<itext>
				<translation lang="en" default="">
					<text id="name-label">
						<value>Name?</value>
					</text>
					<text id="artist-label">
						<value>Artist?</value>
					</text>
					<text id="rating-label">
						<value>Rating?</value>
					</text>
					<text id="rating-5-label">
						<value>*****</value>
					</text>
					<text id="mood-label">
						<value>Mood?</value>
					</text>
					<text id="energy-label">
						<value>Energy?</value>
					</text>
					<text id="energy-4-label">
						<value>****</value>
					</text>
					<text id="energy-3-label">
						<value>***</value>
					</text>
					<text id="energy-2-label">
						<value>**</value>
					</text>
					<text id="energy-1-label">
						<value>*</value>
					</text>
				</translation>
			</itext>
		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><setvalue event="xforms-ready" ref="/data/case/@case_id" value="instance('commcaresession')/session/data/case_id_new_song_0"/><bind calculate="/data/basics/name" nodeset="/data/case/create/case_name"/><bind calculate="/data/meta/userID" nodeset="/data/case/create/owner_id"/><bind calculate="/data/basics/artist" nodeset="/data/case/update/artist" relevant="count(/data/basics/artist) &gt; 0"/><bind calculate="/data/opinions/energy" nodeset="/data/case/update/energy" relevant="count(/data/opinions/energy) &gt; 0"/><bind calculate="/data/opinions/mood" nodeset="/data/case/update/mood" relevant="count(/data/opinions/mood) &gt; 0"/><bind calculate="/data/plays" nodeset="/data/case/update/plays" relevant="count(/data/plays) &gt; 0"/><bind calculate="/data/opinions/rating" nodeset="/data/case/update/rating" relevant="count(/data/opinions/rating) &gt; 0"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/></model>
	</h:head>
	<h:body>
		<group ref="/data/basics" appearance="field-list">
			<input ref="/data/basics/name">
				<label ref="jr:itext('name-label')"/>
			</input>
			<input ref="/data/basics/artist">
				<label ref="jr:itext('artist-label')"/>
			</input>
		</group>
		<group ref="/data/opinions" appearance="field-list">
			<select1 ref="/data/opinions/rating">
				<label ref="jr:itext('rating-label')"/>
				<item>
					<label ref="jr:itext('rating-5-label')"/>
					<value>5</value>
				</item>
				<item>
					<label ref="jr:itext('energy-4-label')"/>
					<value>4</value>
				</item>
				<item>
					<label ref="jr:itext('energy-3-label')"/>
					<value>3</value>
				</item>
				<item>
					<label ref="jr:itext('energy-2-label')"/>
					<value>2</value>
				</item>
				<item>
					<label ref="jr:itext('energy-1-label')"/>
					<value>1</value>
				</item>
			</select1>
			<select1 ref="/data/opinions/mood">
				<label ref="jr:itext('mood-label')"/>
				<item>
					<label ref="jr:itext('energy-4-label')"/>
					<value>4</value>
				</item>
				<item>
					<label ref="jr:itext('energy-3-label')"/>
					<value>3</value>
				</item>
				<item>
					<label ref="jr:itext('energy-2-label')"/>
					<value>2</value>
				</item>
				<item>
					<label ref="jr:itext('energy-1-label')"/>
					<value>1</value>
				</item>
			</select1>
			<select1 ref="/data/opinions/energy">
				<label ref="jr:itext('energy-label')"/>
				<item>
					<label ref="jr:itext('energy-4-label')"/>
					<value>4</value>
				</item>
				<item>
					<label ref="jr:itext('energy-3-label')"/>
					<value>3</value>
				</item>
				<item>
					<label ref="jr:itext('energy-2-label')"/>
					<value>2</value>
				</item>
				<item>
					<label ref="jr:itext('energy-1-label')"/>
					<value>1</value>
				</item>
			</select1>
		</group>
	</h:body>
</h:html>