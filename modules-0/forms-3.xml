<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Add measurement</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/BF874B99-9049-4B1B-BF5D-EDCF0E0240F6" uiVersion="1" version="23" name="Add measurement">
					<hours/>
					<measurements>
						<heart_rate/>
						<dilation/>
						<descent/>
						<contraction_frequency/>
						<contraction_length/>
					</measurements>
				<subcase_0><case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><create><case_name/><owner_id/><case_type>measurement</case_type></create><update><contraction_frequency/><contraction_length/><descent/><dilation/><heart_rate/><hours/></update><index><parent case_type="patient"/></index></case></subcase_0><case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""/><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/>
			<bind nodeset="/data/hours" type="xsd:double" required="true()"/>
			<bind nodeset="/data/measurements"/>
			<bind nodeset="/data/measurements/heart_rate" type="xsd:int"/>
			<bind nodeset="/data/measurements/dilation" type="xsd:double"/>
			<bind nodeset="/data/measurements/descent" type="xsd:double"/>
			<bind nodeset="/data/measurements/contraction_frequency" type="xsd:int"/>
			<bind nodeset="/data/measurements/contraction_length" required="true()" relevant="/data/measurements/contraction_frequency &gt; 0"/>
			<itext>
				<translation lang="en" default="">
					<text id="hours-label">
						<value>How many hours has the patient been in active labor?</value>
					</text>
					<text id="measurements-label">
						<value>Measurements</value>
					</text>
					<text id="heart_rate-label">
						<value>Fetal heart rate (bpm)</value>
					</text>
					<text id="dilation-label">
						<value>Cervical dilation (cm)</value>
					</text>
					<text id="descent-label">
						<value>Descent of head (cm)</value>
					</text>
					<text id="contraction_frequency-label">
						<value>Number of contractions per 10 minutes</value>
					</text>
					<text id="contraction_length-label">
						<value>Length of each contraction</value>
					</text>
					<text id="contraction_length-1-label">
						<value>Less than 20 seconds</value>
					</text>
					<text id="contraction_length-2-label">
						<value>20 - 40 seconds</value>
					</text>
					<text id="contraction_length-3-label">
						<value>Longer than 40 seconds</value>
					</text>
				</translation>
			</itext>
		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><bind calculate="instance('commcaresession')/session/data/case_id" nodeset="/data/case/@case_id"/><bind calculate="/data/meta/timeEnd" nodeset="/data/subcase_0/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/subcase_0/case/@user_id"/><setvalue event="xforms-ready" ref="/data/subcase_0/case/@case_id" value="uuid()"/><bind calculate="/data/hours" nodeset="/data/subcase_0/case/create/case_name"/><bind calculate="/data/meta/userID" nodeset="/data/subcase_0/case/create/owner_id"/><bind calculate="/data/measurements/contraction_frequency" nodeset="/data/subcase_0/case/update/contraction_frequency" relevant="count(/data/measurements/contraction_frequency) &gt; 0"/><bind calculate="/data/measurements/contraction_length" nodeset="/data/subcase_0/case/update/contraction_length" relevant="count(/data/measurements/contraction_length) &gt; 0"/><bind calculate="/data/measurements/descent" nodeset="/data/subcase_0/case/update/descent" relevant="count(/data/measurements/descent) &gt; 0"/><bind calculate="/data/measurements/dilation" nodeset="/data/subcase_0/case/update/dilation" relevant="count(/data/measurements/dilation) &gt; 0"/><bind calculate="/data/measurements/heart_rate" nodeset="/data/subcase_0/case/update/heart_rate" relevant="count(/data/measurements/heart_rate) &gt; 0"/><bind calculate="/data/hours" nodeset="/data/subcase_0/case/update/hours" relevant="count(/data/hours) &gt; 0"/><bind calculate="/data/case/@case_id" nodeset="/data/subcase_0/case/index/parent"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/></model>
	</h:head>
	<h:body>
		<input ref="/data/hours">
			<label ref="jr:itext('hours-label')"/>
		</input>
		<group ref="/data/measurements" appearance="field-list">
			<label ref="jr:itext('measurements-label')"/>
			<input ref="/data/measurements/heart_rate">
				<label ref="jr:itext('heart_rate-label')"/>
			</input>
			<input ref="/data/measurements/dilation">
				<label ref="jr:itext('dilation-label')"/>
			</input>
			<input ref="/data/measurements/descent">
				<label ref="jr:itext('descent-label')"/>
			</input>
			<input ref="/data/measurements/contraction_frequency">
				<label ref="jr:itext('contraction_frequency-label')"/>
			</input>
			<select1 ref="/data/measurements/contraction_length">
				<label ref="jr:itext('contraction_length-label')"/>
				<item>
					<label ref="jr:itext('contraction_length-1-label')"/>
					<value>1</value>
				</item>
				<item>
					<label ref="jr:itext('contraction_length-2-label')"/>
					<value>2</value>
				</item>
				<item>
					<label ref="jr:itext('contraction_length-3-label')"/>
					<value>3</value>
				</item>
			</select1>
		</group>
	</h:body>
</h:html>
