			<bind nodeset="/data/NO" calculate="'no'"/>
			<bind nodeset="/data/had_4_pncs"/>
			<bind nodeset="/data/seen_by_professional" relevant="/data/delivery_status = /data/YES" calculate="if(/data/delivery/delivery_place = 'health_post' or /data/delivery/delivery_place = 'other', /data/YES, /data/NO)"/>
