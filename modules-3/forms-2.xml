		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><bind calculate="instance('commcaresession')/session/data/case_id" nodeset="/data/case/@case_id"/><bind calculate="/data/referral_date_postnatal" nodeset="/data/case/update/referral_date" relevant="count(/data/referral_date_postnatal) &gt; 0"/><bind calculate="/data/trigger_referral_postnatal" nodeset="/data/case/update/trigger_referral" relevant="count(/data/trigger_referral_postnatal) &gt; 0"/><bind calculate="instance('commcaresession')/session/data/usercase_id" nodeset="/data/commcare_usercase/case/@case_id"/><bind calculate="/data/meta/timeEnd" nodeset="/data/commcare_usercase/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/commcare_usercase/case/@user_id"/><bind calculate="/data/referral_date_postnatal" nodeset="/data/commcare_usercase/case/update/referral_date_postnatal" relevant="count(/data/referral_date_postnatal) &gt; 0"/><bind calculate="/data/trigger_referral_postnatal" nodeset="/data/commcare_usercase/case/update/trigger_referral_postnatal" relevant="count(/data/trigger_referral_postnatal) &gt; 0"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instanc