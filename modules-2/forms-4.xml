			<bind nodeset="/data/patient_info_list" relevant="selected(/data/update_info_select/update_info_list, 'patient_info_update')"/>
			<bind nodeset="/data/patient_info_list/name" type="xsd:string" required="true()"/>
			<bind nodeset="/data/patient_info_list/mrd" type="xsd:int" required="true()"/>
			<bind nodeset="/data/patient_info_list/bed" type="xsd:int" required="true()"/>
			<bind nodeset="/data/patient_info_list/wife_of" type="xsd:string" required="true()"/>
			<bind nodeset="/data/patient_info_list/age_input" type="xsd:int" required="true()"/>
