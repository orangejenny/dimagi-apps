			<bind nodeset="/data/high_risk_page/high_risk_score_label"/>
			<bind nodeset="/data/high_risk_page/high_risk_flag" relevant="/data/high_risk_score &gt; 3"/>
			<bind nodeset="/data/age_input" calculate="/data/patient_info_list/age_input"/>
			<bind nodeset="/data/parity" calculate="/data/patient_info_list/parity"/>
			<bind nodeset="/data/age_risk_1" calculate="if(/data/age_input &lt; 18 or /data/age_input &gt; 35, 1, 0)"/>
			<bind nodeset="/data/para_risk_1" calculate="if(/data/patient_info_list/parity = &quot;primi&quot; or /data/patient_info_list/parity = &quot;multi&quot;, 1, 0)"/>
			<bind nodeset="/data/height_risk_1" calculate="if(/data/high_risk_list/height_risk &lt; 145, 1, 0)"/>
			<bind nodeset="/data/weight_risk_3" calculate="if((/data/high_risk_list/weight_risk &lt; 45) or (/data/high_risk_list/weight_risk &gt; 90), 3, 0)"/>
			<bind nodeset="/data/risk_history_2" calculate="if((/data/high_risk_list/prev_outcome = &quot;still_birth&quot;) or (/data/high_risk_list/prev_outcome = &quot;miscarriage&quot;) or (/data/high_risk_list/prev_outcome = &quot;spont_abortion&quot;), 2, 0)"/>
			<bind nodeset="/data/lbw_risk_1" calculate="if(/data/high_risk_list/risk_lbw = &quot;yes&quot;, 1, 0)"/>
			<bind nodeset="/data/risk_uterine_surgery_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;uterine_surgery&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_aph_pph_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;aph_pph&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_manual_placenta_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;manual_placenta&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_anaemia_3" calculate="if(/data/high_risk_list/prev_conditions = &quot;anaemia&quot;, 3, 0)"/>
			<bind nodeset="/data/risk_febrile_ailment_1" calculate="if(/data/high_risk_list/prev_conditions = &quot;febrile_ailment&quot;, 1, 0)"/>
			<bind nodeset="/data/risk_hyptertension_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;hypertension&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_medical_condition_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;medical_condition&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_bleeding_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;bleeding&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_presentation_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;presentation&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_maturity_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;maturity&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_prom_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;prom&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_fetal_distress_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;fetal_distress&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_prolonged_labour_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;prolonged_labour&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_uterine_size_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;uterine_size&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_dai_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;dai&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_pre_eclampsia_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;pre_eclampsia&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_eclampsia_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;eclampsia&quot;, 2, 0)"/>
			<bind nodeset="/data/high_risk_score" calculate="/data/age_risk_1+/data/para_risk_1+/data/height_risk_1+/data/weight_risk_3+/data/risk_history_2+/data/lbw_risk_1+/data/risk_uterine_surgery_2+/data/risk_aph_pph_2+/data/risk_manual_placenta_2+/data/risk_anaemia_3+/data/risk_febrile_ailment_1+/data/risk_hyptertension_2+/data/risk_medical_condition_2+/data/risk_bleeding_2+/data/risk_presentation_2+/data/risk_maturity_2+/data/risk_prom_2+/data/risk_fetal_distress_2+/data/risk_prolonged_labour_2+/data/risk_uterine_size_2+/data/risk_dai_2+/data/risk_pre_eclampsia_2+/data/risk_eclampsia_2"/>
