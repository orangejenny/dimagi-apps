<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Record Delivery</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/75514312-FF32-43BF-A863-F14ED4370546" uiVersion="1" version="1368" name="Record Delivery">
					<delivery_questions>
						<delivery_type/>
						<maternal_outcome/>
						<no_of_babies jr:template="">
							<fetal_outcome/>
							<synto/>
							<placenta/>
							<bleeding/>
							<baby_questions>
								<birth/>
								<cried/>
								<baby_sex/>
								<apgar_score/>
								<weight/>
							</baby_questions>
						</no_of_babies>
					</delivery_questions>
					<paediatrician_name/>
					<number_of_babies/>
					<patient_status/>
					<blank/>
					<screenshots/>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><update><delivery_type/><last_exam_time/><last_pv_time/><maternal_outcome/><number_of_babies/><patient_status/></update></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/><instance id="casedb" src="jr://instance/casedb"/>
			<bind nodeset="/data/delivery_questions"/>
			<bind nodeset="/data/delivery_questions/delivery_type" required="true()"/>
			<bind nodeset="/data/delivery_questions/maternal_outcome"/>
			<bind nodeset="/data/delivery_questions/no_of_babies"/>
			<bind nodeset="/data/delivery_questions/no_of_babies/fetal_outcome"/>
			<bind nodeset="/data/delivery_questions/no_of_babies/synto" required="true()"/>
			<bind nodeset="/data/delivery_questions/no_of_babies/placenta" required="true()"/>
			<bind nodeset="/data/delivery_questions/no_of_babies/bleeding" required="true()"/>
			<bind nodeset="/data/delivery_questions/no_of_babies/baby_questions"/>
			<bind nodeset="/data/delivery_questions/no_of_babies/baby_questions/birth" type="xsd:dateTime"/>
			<bind nodeset="/data/delivery_questions/no_of_babies/baby_questions/cried" required="true()"/>
			<bind nodeset="/data/delivery_questions/no_of_babies/baby_questions/baby_sex" required="false()"/>
			<bind nodeset="/data/delivery_questions/no_of_babies/baby_questions/apgar_score" type="xsd:int"/>
			<bind nodeset="/data/delivery_questions/no_of_babies/baby_questions/weight" type="xsd:double"/>
			<bind nodeset="/data/paediatrician_name" type="xsd:string"/>
			<bind nodeset="/data/number_of_babies"/>
			<bind nodeset="/data/patient_status" calculate="'delivered'"/>
			<bind nodeset="/data/blank" calculate="''"/>
			<bind nodeset="/data/screenshots"/>
			<itext>
				<translation lang="en" default="">
					<text id="delivery_type-label">
						<value>Delivery type:</value>
					</text>
					<text id="delivery_type-normal_vd-label">
						<value>Full-term normal vaginal delivery</value>
					</text>
					<text id="delivery_type-vd_epi-label">
						<value>Normal vaginal delivery with episiotomy</value>
					</text>
					<text id="delivery_type-vd_malpresentation-label">
						<value>Vaginal delivery in malpresentation</value>
					</text>
					<text id="delivery_type-assisted-label">
						<value>Assisted delivery (forceps or vent use)</value>
					</text>
					<text id="delivery_type-cesarean-label">
						<value>cesarean</value>
					</text>
					<text id="delivery_questions/maternal_outcome-label">
						<value>Is the mother alive?</value>
					</text>
					<text id="delivery_questions/no_of_babies-label">
						<value>Number of babies</value>
					</text>
					<text id="delivery_questions/fetal_outcome-label">
						<value>Is the baby alive?</value>
					</text>
					<text id="synto-label">
						<value>Administered 10 units of Synto?</value>
					</text>
					<text id="placenta-label">
						<value>Was the placenta delivered completely?</value>
					</text>
					<text id="bleeding-label">
						<value>Bleeding within normal limits (&lt;500ml)?</value>
					</text>
					<text id="bleeding-yes-label">
						<value>Yes</value>
					</text>
					<text id="bleeding-no-label">
						<value>No</value>
					</text>
					<text id="birth-label">
						<value>Time and date of birth:</value>
					</text>
					<text id="cried-label">
						<value>Baby cried immediately after birth?</value>
					</text>
					<text id="baby_questions/baby_sex-label">
						<value>Sex of the baby:</value>
					</text>
					<text id="baby_questions/baby_sex-female-label">
						<value>Female</value>
					</text>
					<text id="baby_questions/baby_sex-male-label">
						<value>Male</value>
					</text>
					<text id="baby_questions/baby_sex-other-label">
						<value>Other</value>
					</text>
					<text id="apgar_score-label">
						<value>APGAR score:</value>
					</text>
					<text id="weight-label">
						<value>Weight (kg):</value>
					</text>
					<text id="baby_questions/paediatrician_name-label">
						<value>Pediatrician's name:</value>
					</text>
					<text id="screenshots-label">
						<value>Mother has now delivered. Please go to Deliveries and take a screenshot of the completed Partograph. 

Additionally, Please don't forget to take a photo of the paper partograph.</value>
					</text>
				</translation>
				<translation lang="hin">
