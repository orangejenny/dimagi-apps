<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Record Delivery</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/75514312-FF32-43BF-A863-F14ED4370546" uiVersion="1" version="633" name="Record Delivery">
					<delivery_questions>
						<delivery_type/>
						<maternal_outcome/>
						<fetal_outcome/>
						<synto/>
						<placenta/>
						<bleeding/>
					</delivery_questions>
					<patient_status/>
					<baby_questions>
						<birth/>
						<cried/>
						<baby_sex/>
						<apgar_score/>
						<weight/>
						<paediatrician_name/>
					</baby_questions>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><update><patient_status/></update></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/>
			<bind nodeset="/data/delivery_questions"/>
			<bind nodeset="/data/delivery_questions/delivery_type" required="true()"/>
			<bind nodeset="/data/delivery_questions/maternal_outcome"/>
			<bind nodeset="/data/delivery_questions/fetal_outcome"/>
			<bind nodeset="/data/delivery_questions/synto" required="true()"/>
			<bind nodeset="/data/delivery_questions/placenta" required="true()"/>
			<bind nodeset="/data/delivery_questions/bleeding" required="true()"/>
			<bind nodeset="/data/patient_status" calculate="'delivered'"/>
			<bind nodeset="/data/baby_questions"/>
			<bind nodeset="/data/baby_questions/birth" type="xsd:dateTime"/>
			<bind nodeset="/data/baby_questions/cried" required="true()"/>
			<bind nodeset="/data/baby_questions/baby_sex" required="false()"/>
			<bind nodeset="/data/baby_questions/apgar_score" type="xsd:int"/>
			<bind nodeset="/data/baby_questions/weight" type="xsd:double"/>
			<bind nodeset="/data/baby_questions/paediatrician_name"/>
			<itext>
				<translation lang="en" default="">
					<text id="delivery_type-label">
						<value>Delivery type:</value>
					</text>
					<text id="delivery_type-normal_vd-label">
						<value>Full-term normal vaginal delivery</value>
					</text>
					<text id="delivery_type-vd_epi-label">
						<value>Normal vaginal delivery with episiotomy</value>
					</text>
					<text id="delivery_type-vd_malpresentation-label">
						<value>Vaginal delivery in malpresentation</value>
					</text>
					<text id="delivery_type-assisted-label">
						<value>Assisted delivery (forceps or vent use)</value>
					</text>
					<text id="delivery_type-cesarean-label">
						<value>cesarean</value>
					</text>
					<text id="delivery_questions/maternal_outcome-label">
						<value>Is the mother alive?</value>
					</text>
					<text id="delivery_questions/fetal_outcome-label">
						<value>Is the baby alive?</value>
					</text>
					<text id="delivery_questions/fetal_outcome-yes-label">
						<value>Yes</value>
					</text>
					<text id="delivery_questions/fetal_outcome-no-label">
						<value>No</value>
					</text>
					<text id="synto-label">
						<value>Administered 10 units of Synto?</value>
					</text>
					<text id="placenta-label">
						<value>Was the placenta delivered completely?</value>
					</text>
					<text id="bleeding-label">
						<value>Bleeding within normal limits (&lt;500ml)?</value>
					</text>
					<text id="bleeding-yes-label">
						<value>Yes</value>
					</text>
					<text id="bleeding-no-label">
						<value>No</value>
					</text>
					<text id="birth-label">
						<value>Time and date of birth:</value>
					</text>
					<text id="cried-label">
						<value>Baby cried immediately after birth?</value>
					</text>
					<text id="baby_questions/baby_sex-label">
						<value>Sex of the baby:</value>
					</text>
					<text id="baby_questions/baby_sex-female-label">
						<value>Female</value>
					</text>
					<text id="baby_questions/baby_sex-male-label">
						<value>Male</value>
					</text>
					<text id="baby_questions/baby_sex-other-label">
						<value>Other</value>
					</text>
					<text id="apgar_score-label">
						<value>APGAR score:</value>
					</text>
					<text id="weight-label">
						<value>Weight (kg):</value>
					</text>
					<text id="baby_questions/paediatrician_name-label">
						<value>Paediatrician's name:</value>
					</text>
				</translation>
				<translation lang="hin">
					<text id="delivery_type-label">
						<value>&#2346;&#2381;&#2352;&#2360;&#2357; &#2325;&#2366; &#2346;&#2381;&#2352;&#2325;&#2366;&#2352;</value>
					</text>
					<text id="delivery_type-normal_vd-label">
						<value>&#2346;&#2370;&#2352;&#2381;&#2339; &#2309;&#2357;&#2343;&#2367; &#2360;&#2366;&#2350;&#2366;&#2344;&#2381;&#2351; &#2351;&#2379;&#2344;&#2367; &#2346;&#2381;&#2352;&#2360;&#2357;</value>
					</text>
					<text id="delivery_type-vd_epi-label">
						<value>&#2319;&#2346;&#2367;&#2360;&#2367;&#2335;&#2379;&#2350;&#2368; &#2325;&#2352;&#2325;&#2375; &#2360;&#2366;&#2350;&#2366;&#2344;&#2381;&#2351; &#2346;&#2381;&#2352;&#2360;&#2357;</value>
					</text>
					<text id="delivery_type-vd_malpresentation-label">
						<value>&#2351;&#2379;&#2344;&#2368; &#2360;&#2375; &#2346;&#2381;&#2352;&#2360;&#2357; (&#2348;&#2330;&#2381;&#2330;&#2375; &#2325;&#2375; &#2346;&#2376;&#2352; &#2346;&#2361;&#2354;&#2375;)</value>
					</text>
					<text id="delivery_type-assisted-label">
						<value>&#2325;&#2367;&#2360;&#2368; &#2360;&#2361;&#2366;&#2351;&#2340;&#2366; &#2360;&#2375; (&#2325;&#2376;&#2306;&#2330;&#2368;, &#2342;&#2348;&#2366;&#2357;)</value>
