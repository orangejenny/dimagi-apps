						<value>&#2325;&#2381;&#2351;&#2366; &#2350;&#2366;&#2305; &#2332;&#2368;&#2357;&#2367;&#2340; &#2361;&#2376;&#2306;?

</value>
					</text>
					<text id="delivery_questions/fetal_outcome-label">
						<value>&#2325;&#2381;&#2351;&#2366; &#2348;&#2330;&#2381;&#2330;&#2366; &#2332;&#2368;&#2357;&#2367;&#2340; &#2361;&#2376;?
</value>
					</text>
					<text id="delivery_questions/fetal_outcome-yes-label">
						<value>&#2361;&#2366;&#2305;
</value>
					</text>
					<text id="delivery_questions/fetal_outcome-no-label">
						<value>&#2344;&#2361;&#2368;&#2306;
</value>
					</text>
					<text id="synto-label">
						<value>&#2346;&#2381;&#2352;&#2358;&#2366;&#2360;&#2344; 10 &#2351;&#2370;&#2344;&#2367;&#2335;&#2381;&#2360;  of &#2360;&#2381;&#2351;&#2381;&#2344;&#2381;&#2340;&#2379;/&#2342;&#2357;&#2366;&#2312;?</value>
					</text>
					<text id="placenta-label">
						<value>&#2327;&#2352;&#2381;&#2349;&#2344;&#2366;&#2354; &#2346;&#2370;&#2352;&#2368; &#2340;&#2352;&#2361; &#2344;&#2367;&#2325;&#2354;&#2368;?</value>
					</text>
					<text id="bleeding-label">
						<value>&#2360;&#2366;&#2350;&#2366;&#2344;&#2381;&#2351; &#2360;&#2368;&#2350;&#2366; &#2325;&#2375; &#2349;&#2368;&#2340;&#2352; &#2352;&#2325;&#2381;&#2340; &#2360;&#2381;&#2352;&#2366;&#2357; (&lt;500ml)?</value>
					</text>
					<text id="bleeding-yes-label">
						<value>&#2361;&#2366;&#2305;</value>
					</text>
					<text id="bleeding-no-label">
						<value>&#2344;&#2361;&#2368;&#2306;</value>
					</text>
					<text id="birth-label">
						<value>&#2332;&#2344;&#2381;&#2350; &#2325;&#2366; &#2360;&#2350;&#2351; &#2324;&#2352; &#2342;&#2367;&#2344;:</value>
					</text>
					<text id="cried-label">
						<value>&#2325;&#2381;&#2351;&#2366; &#2348;&#2330;&#2381;&#2330;&#2366; &#2332;&#2344;&#2381;&#2350; &#2325;&#2375; &#2340;&#2369;&#2352;&#2306;&#2340; &#2348;&#2366;&#2342; &#2352;&#2379;&#2351;&#2366;</value>
					</text>
					<text id="baby_questions/baby_sex-label">
						<value>Sex of the baby:</value>
					</text>
					<text id="baby_questions/baby_sex-female-label">
						<value>Female</value>
					</text>
					<text id="baby_questions/baby_sex-male-label">
						<value>Male</value>
					</text>
					<text id="baby_questions/baby_sex-other-label">
						<value>Other</value>
					</text>
					<text id="apgar_score-label">
						<value>&#2320; &#2346; &#2327;&#2366; &#2352; &#2360;&#2381;&#2325;&#2379;&#2352;:</value>
					</text>
					<text id="weight-label">
						<value>&#2357;&#2395;&#2344; (&#2325;&#2332;&#2368;):</value>
					</text>
					<text id="baby_questions/paediatrician_name-label">
						<value>Paediatrician's name:</value>
					</text>
				</translation>
			</itext>
		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><bind calculate="instance('commcaresession')/session/data/case_id" nodeset="/data/case/@case_id"/><bind calculate="/data/patient_status" nodeset="/data/case/update/patient_status" relevant="count(/data/patient_status) &gt; 0"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/></model>
	</h:head>
	<h:body>
		<group ref="/data/delivery_questions" appearance="field-list">
			<select1 ref="/data/delivery_questions/delivery_type">
				<label ref="jr:itext('delivery_type-label')"/>
				<item>
					<label ref="jr:itext('delivery_type-normal_vd-label')"/>
					<value>normal_vd</value>
				</item>
				<item>
					<label ref="jr:itext('delivery_type-vd_epi-label')"/>
					<value>vd_epi</value>
				</item>
				<item>
					<label ref="jr:itext('delivery_type-vd_malpresentation-label')"/>
					<value>vd_malpresentation</value>
				</item>
				<item>
					<label ref="jr:itext('delivery_type-assisted-label')"/>
					<value>assisted</value>
				</item>
				<item>
					<label ref="jr:itext('delivery_type-cesarean-label')"/>
					<value>cesarean</value>
				</item>
			</select1>
			<select1 ref="/data/delivery_questions/maternal_outcome">
				<label ref="jr:itext('delivery_questions/maternal_outcome-label')"/>
				<item>
					<label ref="jr:itext('delivery_questions/fetal_outcome-yes-label')"/>
					<value>yes</value>
				</item>
				<item>
					<label ref="jr:itext('delivery_questions/fetal_outcome-no-label')"/>
					<value>no</value>
				</item>
			</select1>
			<select1 ref="/data/delivery_questions/fetal_outcome">
				<label ref="jr:itext('delivery_questions/fetal_outcome-label')"/>
				<item>
					<label ref="jr:itext('delivery_questions/fetal_outcome-yes-label')"/>
					<value>yes</value>
				</item>
				<item>
					<label ref="jr:itext('delivery_questions/fetal_outcome-no-label')"/>
					<value>no</value>
				</item>
			</select1>
			<select1 ref="/data/delivery_questions/synto">
				<label ref="jr:itext('synto-label')"/>
				<item>
					<label ref="jr:itext('bleeding-yes-label')"/>
					<value>yes</value>
				</item>
				<item>
					<label ref="jr:itext('bleeding-no-label')"/>
					<value>no</value>
				</item>
			</select1>
			<select1 ref="/data/delivery_questions/placenta">
				<label ref="jr:itext('placenta-label')"/>
				<item>
					<label ref="jr:itext('bleeding-yes-label')"/>
					<value>yes</value>
				</item>
				<item>
					<label ref="jr:itext('bleeding-no-label')"/>
					<value>no</value>
				</item>
			</select1>
			<select1 ref="/data/delivery_questions/bleeding">
				<label ref="jr:itext('bleeding-label')"/>
				<item>
					<label ref="jr:itext('bleeding-yes-label')"/>
					<value>yes</value>
				</item>
				<item>
					<label ref="jr:itext('bleeding-no-label')"/>
					<value>no</value>
				</item>
			</select1>
		</group>
		<group ref="/data/baby_questions" appearance="field-list">
			<input ref="/data/baby_questions/birth">
				<label ref="jr:itext('birth-label')"/>
			</input>
			<select1 ref="/data/baby_questions/cried">
				<label ref="jr:itext('cried-label')"/>
				<item>
					<label ref="jr:itext('bleeding-yes-label')"/>
					<value>yes</value>
				</item>
				<item>
					<label ref="jr:itext('bleeding-no-label')"/>
					<value>no</value>
				</item>
			</select1>
			<select1 ref="/data/baby_questions/baby_sex">
				<label ref="jr:itext('baby_questions/baby_sex-label')"/>
				<item>
					<label ref="jr:itext('baby_questions/baby_sex-female-label')"/>
					<value>female</value>
				</item>
				<item>
					<label ref="jr:itext('baby_questions/baby_sex-male-label')"/>
					<value>male</value>
				</item>
				<item>
					<label ref="jr:itext('baby_questions/baby_sex-other-label')"/>
					<value>other</value>
				</item>
			</select1>
			<input ref="/data/baby_questions/apgar_score">
				<label ref="jr:itext('apgar_score-label')"/>
			</input>
			<input ref="/data/baby_questions/weight">
				<label ref="jr:itext('weight-label')"/>
			</input>
			<trigger ref="/data/baby_questions/paediatrician_name" appearance="minimal">
				<label ref="jr:itext('baby_questions/paediatrician_name-label')"/>
			</trigger>
		</group>
	</h:body>
</h:html>